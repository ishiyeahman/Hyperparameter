    "import optuna\n",
    " \n",
    "def objective(trial):\n",
    "    x = trial.suggest_float(\"x\", -10, 10)\n",
    "    return (x - 2) ** 2\n",
    " \n",
    "study = optuna.create_study()\n",
    "study.optmize(objective, n_trials=100)\n",
    "print(study.best_trial)"
   ]
  }
